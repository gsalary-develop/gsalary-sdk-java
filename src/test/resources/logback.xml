<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <property name="CONSOLE_LOG_PATTERN" value="[%d{yyyy-MM-dd'T'HH:mm:ss.SSS}] [%thread] %-5level %logger: %msg%n"/>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>${CONSOLE_LOG_PATTERN}</pattern>
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>